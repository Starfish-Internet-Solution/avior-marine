			<link_title>accessibility</link_title>
			<link_text>Accessibility</link_text>
		</page>
		<!-- ***************************************************************************************** -->
		<page frontend="hide" cms="hide">
			<page_id>design</page_id>
			<url_name>design</url_name>
			<link_title>starfish internet solutions</link_title>
			<link_text>Web Design</link_text>
		</page>
		<!-- ***************************************************************************************** -->
		
	</navigation_group>
	
</pages>
