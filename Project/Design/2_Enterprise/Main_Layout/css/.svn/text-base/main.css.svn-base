body{color:#F6F6F6;background-color:#242424}body #wrapper{width:1200px;margin:0 auto}body #wrapper #container_wrapper{min-height:400px;padding:1.250em 0}body #wrapper #container_wrapper #container{width:100%}.sprite{background-repeat:no-repeat;background-color:transparent;background-image:url("/Project/Design/2_Enterprise/Main_Layout/images/enterprise_buttons_and_icons_sprite.png")}.sprite2{background-repeat:no-repeat;background-color:transparent;background-image:url("/Project/Design/2_Enterprise/Main_Layout/images/enterprise_buttons_and_icons_sprite3.png")}div[unselectable="on"]{color:black;font-size:10px !important}div.selector select{height:22px}div.selector,div.selector span,div.checker span,div.radio span,div.uploader,div.uploader span.action,div.button,div.button span{background-image:url("/Project/Design/2_Enterprise/Main_Layout/images/sprite.png")}input.text,input.email,input.password,textarea.uniform{background:url("/Project/Design/2_Enterprise/Main_Layout/images/bg-input-focus.png") repeat-x 0px 0px #fff;background:url("/Project/Design/2_Enterprise/Main_Layout/images/bg-input.png") repeat-x 0px 0px #fff}input.text:focus,input.email:focus,input.password:focus,textarea.uniform:focus{background:url("/Project/Design/2_Enterprise/Main_Layout/images/bg-input-focus.png") repeat-x 0px 0px #fff}div.checker{float:left}input[type="submit"][name*="save"],input[type="submit"][name*="delete"]{font-size:0px}#global_loader{left:50%;margin:-55px 0 0 -55px;top:50%;z-index:999}[class*="_dialog"] input[type="text"][name="title"],[class*="_dialog"] input[name*="_url"]{width:300px;margin:-4px 10px 0px 10px;height:25px;color:#333333;border-radius:0}[class*="_dialog"] input:disabled{color:#AAAAAA !important;cursor:default}.category_dialog{width:380px;height:175px;margin-left:-190px;padding:25px 10px 10px}.category_dialog,.subcategory_dialog,.product_dialog{background-image:url("/Project/Design/2_Enterprise/Applications/Products/Main_App_Layout/images/Enterprise-Pop-ups-add.png")}.subcategory_dialog,.product_dialog{width:410px;margin-left:-205px;padding:25px 10px 10px}
